srcdir = @srcdir@
top_srcdir = @top_srcdir@
builddir = @builddir@
top_builddir = @top_builddir@


name = onewire

files = \
	OneWire.cpp

sources = $(addprefix $(srcdir)/,$(files))
objects = $(addsuffix .o, $(files))

clean_files = \
	$(objects) \
	lib$(name).a


.PHONY: all
all: lib$(name).a

lib$(name).a: $(objects)

.PHONY: clean
clean:
	@$(call clean,$(clean_files))


include $(top_builddir)/arduino.mk
include $(top_builddir)/include.mk

